{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"]
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "better-docs": {
      "name": "Eva Frondend Challenge"
    }
  },
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/better-docs"
  }
}
